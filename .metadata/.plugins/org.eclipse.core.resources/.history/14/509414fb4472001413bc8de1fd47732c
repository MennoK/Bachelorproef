package model;

public enum Activity {
	
	WALKING(1, "walking", "wandelen"),
	RUNNING(2, "running", "lopen"),
	STAIRS_UP(3, "walking up stairs", "trap op wandelen"),
	STAIRS_DOWN(4, "walkign down stairs", "trap af wandelen"),
	CYCLING(5, "cycling", "fietsen");
	// TODO: dit lijste aanvullen
	
	Activity(int id, String nameEN, String nameNL) {
		this.id = id;
		this.nameEN = nameEN;
		this.nameNL = nameNL;
	}
	
	public final int id;
	public final String nameEN, nameNL;

}
