/**
 * Alle methodes met verschillende opties opgesomd in een 'enum'
 */
public enum Method {
	
	J48_1("-C 0.25", "-M 2"), 		// pruned met confidence factor 0.25, min. 2 obj. per blad = default
	J48_4("-U", "-M 2"); 			// unpruned, min. 2 obj. per blad

	Method(String... options) {
		this.options = options;
	}
	
	public final String[] options;
	
}
